{
  "name": "mavlink-mappings",
  "version": "1.0.8-20220319-1",
  "author": "Matthias Hryniszak <padcom@gmail.com>",
  "license": "LGPL",
  "description": "MavLink message definitions",
  "keywords": [
    "mavlink",
    "node-mavlink"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/padcom/mavlink-mappings.git"
  },
  "bugs": {
    "email": "padcom@gmail.com",
    "url": "https://github.com/padcom/mavlink-mappings/issues"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/padcom"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "transpile": "tsc",
    "code-generator": "ts-node code-generator.ts",
    "get:minimal": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/minimal.xml -O minimal.xml",
    "get:common": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/common.xml -O common.xml",
    "get:ardupilotmega": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/ardupilotmega.xml -O ardupilotmega.xml",
    "get:uavionix": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/uAvionix.xml -O uavionix.xml",
    "get:icarous": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/icarous.xml -O icarous.xml",
    "get:asluav": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/ASLUAV.xml -O asluav.xml",
    "get:development": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/development.xml -O development.xml",
    "get:matrixpilot": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/matrixpilot.xml -O matrixpilot.xml",
    "get:paparazzi": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/paparazzi.xml -O paparazzi.xml",
    "get:ualberta": "nwget https://raw.githubusercontent.com/mavlink/mavlink/master/message_definitions/v1.0/ualberta.xml -O ualberta.xml",
    "regenerate": "npm-run-all -p 'get:*' && npm run code-generator && npm run transpile",
    "prepublishOnly": "npm install && npm run regenerate"
  },
  "devDependencies": {
    "@types/node": "^15.0.1",
    "@types/xml2js": "^0.4.8",
    "npm-run-all": "^4.1.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.3",
    "wget-improved": "^3.2.1",
    "xml2js": "^0.4.23"
  }
}
